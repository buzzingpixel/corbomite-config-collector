{
    "name": "buzzingpixel/corbomite-config-collector",
    "version": "1.1.1",
    "description": "Corbomite Config Collector",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "buzzingpixel/corbomite-di": "^1.2"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.1",
        "doctrine/coding-standard": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\configcollector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "corbomite\\tests\\": "tests/"
        }
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php"
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
